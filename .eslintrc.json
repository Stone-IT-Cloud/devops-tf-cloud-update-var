{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": ["warn"],
        "no-console": "off",
        "no-undef": "error",
        "no-var": "error",
        "prefer-const": "error"
    }
}
